---
import Layout from "../layouts/Layout.astro";
import Calculator from "../components/CalculatorComp.astro";
import Modal from "../components/Modal.astro";
const modal =
  "Need to know a caller or involved party's age in a hurry? Just enter their birth year, and their age will be given back to you instantly based off of the current date.";
---

<Layout>
  <Modal content={modal} />
  <Calculator transition:animate="slide" />
</Layout>
