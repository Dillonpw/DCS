---
import Head from "../components/Head.astro";
import Header from "../components/Header.astro";
import Footer from "../components/Footer.astro";
import "../styles/styles.css";

interface Props {
  title: string;
}

const { title } = Astro.props;
---
<html lang="en">
<Head title={title} />
<body>
  <Header />
  <h1>{title}</h1>
  <slot id="content" />
  <Footer />
</body>
</html>
