---
interface Props {
  title: string;
}
import Header from "../components/Header.astro";
import Footer from "../components/Footer.astro";
import "../styles/styles.css";

const { title } = Astro.props;
---

<html lang="en">
  <head>
    <meta charset="UTF-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <meta name="description" content="Simplifiy your job as a 911 Dispatcher" />
    <meta
      name="keywords"
      content="911 Dispatcher Resources, Emergency Services Tools, Dispatcher Schedule Tracker, Public Safety Checklist, Live Weather Updates, Emergency Response Weather App, Age Calculation Tool, Dispatcher Duty Planner, First Responder Support, Emergency Dispatch Tools, Shift Scheduling for Dispatchers, Public Safety Software, Incident Management Tools, Dispatcher Workflow Optimization, Real-Time Weather for Emergency Services, Professional Tools for 911 Dispatchers, Emergency Services Age Calculator, Task Checklist for Dispatchers, Public Safety Communication Tools, Emergency Response Planning Tools"
    />
    <meta name="google-adsense-account" content="ca-pub-4500026491096816" />

    <meta name="robots" content="index, follow" />
    <link rel="canonical" href="https://dcs-psi.vercel.app/" />
    <link rel="stylesheet" href="src/styles/styles.css" />
    <link rel="preconnect" href="https://fonts.googleapis.com" />
    <link rel="preconnect" href="https://fonts.gstatic.com" crossorigin />
    <link rel="icon" type="image/svg+xml" href="/logo.svg" />
    <link
      href="https://fonts.googleapis.com/css2?family=Amatic+SC:wght@700&family=Montserrat:wght@700&family=Orbitron&family=Poppins:wght@400;500;600;700&family=Rowdies:wght@700&family=Ubuntu:ital,wght@0,400;1,700&display=swap"
      rel="stylesheet"
    />
    <meta name="generator" content={Astro.generator} />
    <title>{title}</title>
  </head>
  <body>
    <Header />
    <h1>{title}</h1>
    <slot id="content" />
    <Footer />
  </body>
</html>
